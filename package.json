  "scripts": {
    "dev": "set NODE_OPTIONS=--dns-result-order=ipv4first && cross-env NEXT_PRIVATE_OUTPUT_TRACE_WORKER=0 NEXT_DIST_DIR=.next next dev",
    "dev:legacy-tls": "cross-env NODE_OPTIONS=--openssl-legacy-provider next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "diagnostics:100ms": "node scripts/check-100ms-env.js",
    "diagnostics:routes": "node scripts/check-duplicate-routes.mjs",
    "env:check": "node scripts/check-required-env.mjs",
    "diagnostics:route-stubs": "node scripts/check-duplicate-route-stubs.mjs",
    "build:strict": "npm run env:check && next build",
    "hms:gen-mgmt-token": "node scripts/generate-hms-management-token.js",
    "hms:rotate": "npm run hms:gen-mgmt-token -- --ttl 7200",
    "scan:secrets": "node scripts/scan-secrets.js",
    "prepare": "git config core.hooksPath .githooks || true"
  },
